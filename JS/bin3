let user = {
  firstname: "Charles",
  lastName: "Agnew",
  age: 32,
  email: "charles.agnew@telnet.com"
};
for (let key in user) {
  console.log(key);
  console.log(user.fistname);
  console.log(user[firstname]);
}
for (let key in user) {
  console.log(`${key} -> ${user[key]}`);
}